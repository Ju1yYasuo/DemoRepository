<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.bi.mapper.EnvironmentInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.entity.bi.EnvironmentInfo">
        <id column="id" property="id"/>
        <result column="deleted" property="deleted"/>
        <result column="remarks" property="remarks"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="pm_two_five" property="pmTwoFive"/>
        <result column="pm_ten" property="pmTen"/>
        <result column="wind_speed" property="windSpeed"/>
        <result column="noise" property="noise"/>
        <result column="humidity" property="humidity"/>
        <result column="temperature" property="temperature"/>
        <result column="muck_protection_rate" property="muckProtectionRate"/>
        <result column="topsoil_protection_rate" property="topsoilProtectionRate"/>
        <result column="environmental_rectification_rate" property="environmentalRectificationRate"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        deleted,
        remarks,
        create_time,
        update_time,
        pm_two_five, pm_ten, wind_speed, noise, humidity, temperature, muck_protection_rate, topsoil_protection_rate, environmental_rectification_rate
    </sql>

</mapper>
